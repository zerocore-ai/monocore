{
  "name": "microsandbox/microsandbox",
  "description": "A minimal PHP SDK for the Microsandbox project",
  "type": "library",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Microsandbox Team",
      "email": "team@microsandbox.dev"
    }
  ],
  "homepage": "https://github.com/microsandbox/microsandbox",
  "support": {
    "issues": "https://github.com/microsandbox/microsandbox/issues",
    "source": "https://github.com/microsandbox/microsandbox"
  },
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Microsandbox\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Microsandbox\\Tests\\": "tests/"
    }
  }
}
